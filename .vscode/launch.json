    {
      "name": "A1 debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/src/threads/build/kernel.o",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceRoot}/src/threads/build/",
      "MIMode": "gdb",
      "miDebuggerServerAddress": "localhost:1234",
